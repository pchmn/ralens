{
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "enterpriseProvisioning": "adhoc"
      },
      "env": {
        "APP_ENV": "development",
        "EXPO_PUBLIC_NHOST_AUTH_URL": "<NHOST_AUTH_URL_DEV>",
        "EXPO_PUBLIC_NHOST_GRAPHQL_URL": "<NHOST_GRAPHQL_URL_DEV>",
        "EXPO_PUBLIC_NHOST_STORAGE_URL": "<NHOST_STORAGE_URL_DEV>",
        "EXPO_PUBLIC_NHOST_FUNCTIONS_URL": "<NHOST_FUNCTIONS_URL_DEV>"
      }
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "enterpriseProvisioning": "adhoc"
      },
      "env": {
        "APP_ENV": "preview",
        "EXPO_PUBLIC_NHOST_AUTH_URL": "<NHOST_AUTH_URL_DEV>",
        "EXPO_PUBLIC_NHOST_GRAPHQL_URL": "<NHOST_GRAPHQL_URL_DEV>",
        "EXPO_PUBLIC_NHOST_STORAGE_URL": "<NHOST_STORAGE_URL_DEV>",
        "EXPO_PUBLIC_NHOST_FUNCTIONS_URL": "<NHOST_FUNCTIONS_URL_DEV>"
      }
    },
    "production": {
      "channel": "production",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "production",
        "EXPO_PUBLIC_NHOST_AUTH_URL": "<NHOST_AUTH_URL_PROD>",
        "EXPO_PUBLIC_NHOST_GRAPHQL_URL": "<NHOST_GRAPHQL_URL_PROD>",
        "EXPO_PUBLIC_NHOST_STORAGE_URL": "<NHOST_STORAGE_URL_PROD>",
        "EXPO_PUBLIC_NHOST_FUNCTIONS_URL": "<NHOST_FUNCTIONS_URL_PROD>"
      }
    }
  }
}
