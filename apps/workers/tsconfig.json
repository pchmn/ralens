{
  "extends": "@ralens/tsconfig/base.json",
  "include": ["src"],
  "exclude": ["node_modules", "dist", "coverage", "src/__test__"],
  "compilerOptions": {
    "downlevelIteration": true,
    "lib": [
      "ES2020"
    ],
    "types": ["@cloudflare/workers-types"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
